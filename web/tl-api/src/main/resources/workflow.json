[
	{
		"id": "WF1001",
		"name": "大额可疑交易上报工作流程",
		"states": [
			{
				"state": "SISP_PRE_DEAL",
				"stateName": "可疑待处理",
				"actions":[
					{"action":"DEAL","name":"处理","style":"layui-btn-normal","fun":"deal"},
					{"action":"STOP","name":"结束","event":"STOP","style":"layui-btn-danger"}
				],
				"events": [
					"SISP_DEAL",
					"STOP"
				]
			},
			{
				"state": "SISP_PRE_VERIFY",
				"stateName": "可疑待审核",
				"actions":[
					{"action":"VERIFY","name":"审核","style":"layui-btn-danger","fun":"sispverify"}
				],
				"events": [
					"SISP_VERIFY_PASS",
					"SISP_VERIFY_NOT_PASS",
					"STOP"
				]
			},
			{
				"state": "PRE_RECROD",
				"stateName": "待补录",
				"actions":[
					{"action":"EDIT","name":"补录","style":"layui-btn-normal","fun":"edit"}
				],
				"events": [
					"RECROD_SAVE",
					"SUBMIT"
				]
			},
			{
				"state": "PRE_SUBMIT",
				"stateName": "待提交",
				"actions":[
					{"action":"EDIT","name":"补录","style":"layui-btn-normal","fun":"edit"},
					{"action":"SUBMIT","name":"提交","event":"SUBMIT"},
					{"action":"COPY","name":"复制","style":"layui-btn-normal","fun":"copy"}
				],
				"events": [
					"RECROD_SAVE",
					"SUBMIT"
				]
			},
			{
				"state": "PRE_VERIFY",
				"stateName": "待审核",
				"actions":[
					{"action":"VERIFY","name":"审核","style":"layui-btn-danger","fun":"verify"},
					{"action":"COPY","name":"复制","style":"layui-btn-normal","fun":"copy"}
				],
				"events": [
					"VERIFY_PASS",
					"VERIFY_NOT_PASS"
				]
			},
			{
				"state": "PRE_EXPORT",
				"stateName": "待导出",
				"actions":[
					{"action":"SHOW","name":"查看","fun":"show"},
					{"action":"CANCEL_VERIFY","name":"取消审核","event":"CANCEL_VERIFY","style":"layui-btn-danger"},
					{"action":"COPY","name":"复制","style":"layui-btn-normal","fun":"copy"}
				],
				"events": [
					"CANCEL_VERIFY",
					"EXPORT"
				]
			},
			{
				"state": "PRE_EXPORTOVER",
				"stateName": "已导出",
				"actions":[
					{"action":"SHOW","name":"查看","fun":"show"},
					{"action":"COPY","name":"复制","style":"layui-btn-normal","fun":"copy"}
				],
				"events": [
				    "ADD_STORAGE",
					"RESET",
					"REPORT_AGAIN"
				]
			},
			{
				"state": "PRE_STORAGE",
				"stateName": "已入库",
				"actions":[
					{"action":"SHOW","name":"查看","fun":"show"},
					{"action":"CHANGE","name":"变更","style":"layui-btn-normal","fun":"change"},
					{"action":"COPY","name":"复制","style":"layui-btn-normal","fun":"copy"}
				],
				"events": [
					"CHANGE",
					"DELETE"
				]
			},
			{
				"state": "SISP_PRE_END",
				"stateName": "已终止",
				"actions":[
					{"action":"RESAVE","name":"启用","fun":"show"}
				],
				"events": [
					"RESAVE"
				]
			}
			
		],
		"events": [
			{
				"event": "SISP_DEAL",
				"eventName": "提交",
				"nextState": "SISP_PRE_VERIFY",
				"style":"layui-btn-normal",
				"fun":"deal"
			},
			{
				"event": "SISP_VERIFY_PASS",
				"eventName": "审核",
				"nextState": "PRE_RECROD",
				"fun":"sisppass"
			},
			{
				"event": "SISP_VERIFY_NOT_PASS",
				"eventName": "审核不通过",
				"nextState": "SISP_PRE_DEAL",
				"commentsRequired": true,
				"fun":"sisppassnot"
			},
			{
				"event": "RECROD_SAVE",
				"eventName": "保存",
				"nextState": "PRE_SUBMIT",
				"fun":"save"
			},
			{
				"event": "SUBMIT",
				"eventName": "提交",
				"nextState": "PRE_VERIFY",
				"fun":"submit"
			},
			{
				"event": "VERIFY_PASS",
				"eventName": "审核通过",
				"nextState": "PRE_EXPORT",
				"fun":"pass"
			},
			{
				"event": "VERIFY_NOT_PASS",
				"eventName": "不通过",
				"nextState": "PRE_SUBMIT",
				"commentsRequired": true,
				"fun":"passnot"
			},
			{
				"event": "EXPORT",
				"eventName": "导出",
				"nextState": "PRE_EXPORTOVER",
				"visible":false,
				"fun":"export"
			},
			{
				"event": "ADD_STORAGE",
				"eventName": "入库",
				"nextState": "PRE_STORAGE",
				"fun":"addstorage"
			},
			{
				"event": "REPORT_AGAIN",
				"eventName": "重发",
				"nextState": "PRE_SUBMIT",
				"fun":"reportagain"
			},
			{
				"event": "RESET",
				"eventName": "重置",
				"nextState": "PRE_EXPORT",
				"fun":"reset"
			},
			{
				"event": "DELETE",
				"eventName": "删除",
				"nextState": "PRE_VERIFY",
				"fun":"delete"
			},
			{
				"event": "STOP",
				"eventName": "结束",
				"nextState": "SISP_PRE_END",
				"fun":"stop"
			},
			{
				"event": "RESAVE",
				"eventName": "启用",
				"nextState": "SISP_PRE_DEAL",
				"fun":"resave"
			},
			{
				"event": "CANCEL_VERIFY",
				"eventName": "取消审核",
				"nextState": "PRE_SUBMIT",
				"fun":"cancelverify"
			},
			{
				"event": "CHANGE",
				"eventName": "变更",
				"nextState": "PRE_SUBMIT",
				"fun":"change"
			}
		]
	},
	{
		"id": "WF1002",
		"name": "基本信息工作流程",
		"states": [
			
			
			{
				"state": "PRE_RECROD",
				"stateName": "待补录",
				"actions":[
					{"action":"EDIT","name":"补录","fun":"edit"}
				],
				"events": [
					"RECROD_SAVE"
				]
			},
			{
				"state": "PRE_APPROVE",
				"stateName": "待审核",
				"actions":[
					{"action":"APPROVE","name":"审核","fun":"approve"}
				],
				"events": [
					"APPROVE_PASS",
					"APPROVE_NOT_PASS"
				]
			},
			{
				"state": "APPROVED",
				"stateName": "已审核",
				"actions":[
					{"action":"UPDATE","name":"变更","fun":"update"}
				],
				"events": [
					"UPDATE",
					"UNAPPROVE"
				]
			}
			
		],
		"events": [
			{
				"event": "RECROD_SAVE",
				"eventName": "保存",
				"nextState": "PRE_APPROVE",
				"fun":"recrodsave"
			},
			{
				"event": "APPROVE_PASS",
				"eventName": "审核通过",
				"nextState": "APPROVED",
				"fun":"pass"
			},
			{
				"event": "APPROVE_NOT_PASS",
				"eventName": "不通过",
				"nextState": "PRE_RECROD",
				"commentsRequired": true,
				"fun":"passnot"
			},
			{
				"event": "UPDATE",
				"eventName": "变更",
				"nextState": "PRE_RECROD",
				"fun":"update"
			},
			{
				"event": "UNAPPROVE",
				"eventName": "取消审核",
				"nextState": "PRE_APPROVE",
				"fun":"unappove"
			}		
		]
	}
]
